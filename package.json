{
  "name": "empoze",
  "version": "1.0.0",
  "description": "Compose emails from react and tailwind",
  "main": "index.js",
  "config": {
    "dist": "dist",
    "deploy": "deploy",
    "src": "src"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "pack": "webpack",
    "css": "mailwind --input-html ./deploy/email.html --output-css ./dist/email.css",
    "css-inline": "mailwind --input-html ./deploy/email.html --output-html ./dist/email.html",
    "deploy": "node ./deploy/app.js ",
    "build": "npm run pack && npm run deploy",
    "build-all": "npm run build && npm run css-inline && npm run build && npm run css-inline",
    "prod": "npm run pack && npm run deploy && npm run css-inline"
  },
  "keywords": [
    "email",
    "tailwind",
    "react"
  ],
  "author": "Kolism",
  "license": "MIT",
  "devDependencies": {
    "react-dom": "^17.0.2",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "juice": "^8.0.0",
    "react": "^17.0.2",
    "regenerator-runtime": "^0.11.1",
    "webpack": "^5.68.0"
  }
}
